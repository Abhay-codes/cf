#https://codeforces.com/problemset/problem/1716/A
for _ in range(int(input())):
    n =int(input())
    k =n%3
    if n==1:
        print(2)
        continue 
    if n ==2:
        print(1)
        continue 
    
    if k==0:
        print(n//3)
    elif k ==1:
        a=(n-4)//3  +2 
        print(a)
    else:
        print(n//3+1)
        
    
        
        
